"/api/admin/metadata/{Product}": {
      "put": {
        "summary": "Update Metadata",
        "description": "Update metadata for a product. Only admin users or the owner of the product can update metadata.",
        "tags": ["Metadata"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "Product",
            "description": "The product for which metadata needs to be updated.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "body",
            "name": "metadata",
            "description": "Metadata object to update",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "Product": {
                  "type": "string",
                  "description": "The product ID associated with the metadata."
                },
                "title": {
                  "type": "string",
                  "description": "Title of the metadata."
                },
                "Category": {
                  "type": "string",
                  "description": "Category of the metadata."
                },
                "Geography": {
                  "type": "string",
                  "description": "Geographical area covered by the metadata."
                },
                "Frequency": {
                  "type": "string",
                  "description": "Frequency of the data collection."
                },
                "TimePeriod": {
                  "type": "string",
                  "description": "Time period covered by the data."
                },
                "DataSource": {
                  "type": "string",
                  "description": "Source of the data."
                },
                "Description": {
                  "type": "string",
                  "description": "Detailed description of the data."
                },
                "lastUpdateDate": {
                  "type": "string",
                  "format": "date",
                  "description": "Date of the last update."
                },
                "FutureRelease": {
                  "type": "string",
                  "description": "Future release information."
                },
                "BasePeriod": {
                  "type": "string",
                  "description": "Base period for the data."
                },
                "Keystatistics": {
                  "type": "string",
                  "description": "Key statistics included in the data."
                },
                "NMDS": {
                  "type": "string",
                  "description": "Compliance with the National Metadata Structure."
                },
                "nmdslink": {
                  "type": "string",
                  "format": "url",
                  "description": "Link to NMDS compliance documentation."
                },
                "abc": {
                  "type": "string",
                  "description": "Additional custom field."
                }
              },
              "example": {
                "Product": "cpi",
                "title": "Annual Survey of Industries (ASI)",
                "Category": "Industrial Statistics",
                "Geography": "All India",
                "Frequency": "Annual",
